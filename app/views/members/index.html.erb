		<th>Join Date</th>
	</tr>
	<% @members.each do |member| %>
	<tr>
		<td><%= member.batch  %></td>
		<td><%= member.form_completed  %></td>
		<td><%= member.status  %></td>
		<td><%= member.memb  %></td>
		<td><%= member.first_name  %></td>
		<td><%= member.last_name  %></td>
		<td><%= member.password_digest %></td>
		<td><%= member.admin %></td>
		<td><%= member.address1  %></td>
		<td><%= member.address2  %></td>
		<td><%= member.address3  %></td>
		<td><%= member.post_code  %></td>
		<td><%= member.city  %></td>
		<td><%= member.area  %></td>
		<td><%= member.home  %></td>
		<td><%= member.mobile  %></td>
		<td><%= member.nationality  %></td>
		<td><%= member.email  %></td>
		<td><%= member.emailers  %></td>
		<td><%= member.hardmail  %></td>
		<td><%= member.working  %></td>
