		<td><%= member.computer_skills  %></td>
		<td><%= member.skills_finance  %></td>
		<td><%= member.skills_eventmgmt  %></td>
		<td><%= member.interview  %></td>
		<td><%= member.other_skills  %></td>
		<td><%= member.partner  %></td>
		<td><%= member.partner_first_name  %></td>
		<td><%= member.partner_last_name  %></td>
		<td><%= member.partner_company  %></td>
		<td><%= member.dob  %></td>
		<td><%= member.language  %></td>
		<td><%= member.gender  %></td>
		<td><%= member.join_date  %></td>
	</tr>
	<% end %>
	</thead>
</table>
<div>
	<h4>Import that data!</h4>
	<%= form_tag import_members_path, multipart: true do %>
		<%= file_field_tag :file %>
		<%= submit_tag "Import CSV" %>
	<% end %>
</div>









