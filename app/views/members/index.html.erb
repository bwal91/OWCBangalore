<%= flash[:notice] %>

<table>
	<thead>
	<tr>
		<th>Batch</th>
		<th>Form?</th>
		<th>Status</th>
		<th>Member#</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Password</th>
		<th>Admin</th>
		<th>Address1</th>
		<th>Address2</th>
		<th>Address3</th>
		<th>Post Code</th>
		<th>City</th>
		<th>Area</th>
		<th>Home#</th>
		<th>Mobile#</th>
		<th>Nationality</th>
